# Domain configuration
API_DOMAIN=api.folklovers.example.com
FRONTEND_DOMAIN=folklovers.example.com

# PostgreSQL
POSTGRES_USER=folklovers
POSTGRES_PASSWORD=your_secure_postgres_password_here
